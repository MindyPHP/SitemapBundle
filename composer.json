{
    "name": "mindy/sitemap-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\SitemapBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\SitemapBundle\\Tests\\": ""
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "mindy/sitemap": ">=4.0.3",
        "symfony/console": "~3.0 || ~4.0"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/yaml": "^3.4"
    }
}
